<!DOCTYPE html>
<html>
<head>
  <title>Song List</title>
</head>
<body>
  <h1>Songs</h1>

  <% if (songs.length === 0) { %>
    <p>No songs found.</p>
  <% } else { %>
    <table border="1">
      <tr>
        <th>Title</th>
        <th>Artist</th>
        <th>Rating</th>
      </tr>
      <% songs.forEach(song => { %>
        <tr>
          <td><%= song.title %></td>
          <td><%= song.artist %></td>
          <td><%= song.rating %></td>
        </tr>
      <% }) %>
    </table>
  <% } %>

</body>
</html>